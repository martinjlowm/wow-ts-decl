{
  "name": "@martinjlowm/wow-ts-decl",
  "version": "0.0.1",
  "type": "module",
  "workspaces": ["docs/snippets"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjlowm/wow-ts-decl.git"
  },
  "author": "martinjlowm <martinjlowm@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "development": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js",
      "development": "./src/*.ts"
    }
  },
  "imports": {
    "#@/*": {
      "development": ["./src/*", "./src/*.ts"],
      "default": ["./dist/*", "./dist/*.js"]
    },
    "#$/*": {
      "default": ["./test/*", "./test/*.ts"]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/luaparse": "^0.2.12",
    "@types/node": "^22.10.6",
    "@typescript-to-lua/language-extensions": "^1.19.0",
    "lodash": "^4.17.21",
    "luaparse": "^0.3.1",
    "playwright": "^1.49.1",
    "playwright-cache.ts": "^1.0.1",
    "ts-pattern": "^5.6.0",
    "typescript": "^5.7.3",
    "typescript-language-server": "^4.3.3"
  }
}
