{
  "name": "@martinjlowm/wow-ts-decl",
  "version": "0.0.1",
  "type": "module",
  "workspaces": ["docs/snippets", "typings/extensions"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjlowm/wow-ts-decl.git"
  },
  "author": "martinjlowm <martinjlowm@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "development": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js",
      "development": "./src/*.ts"
    }
  },
  "imports": {
    "#@/*.js": {
      "development": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "#@/*": {
      "development": "./src/*",
      "default": "./dist/*"
    },
    "#$/*.js": {
      "default": "./test/*.ts"
    },
    "#$/*": {
      "default": "./test/*"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/luaparse": "^0.2.12",
    "@types/node": "^22.10.6",
    "@types/semver": "^7",
    "@types/yargs": "^17.0.33",
    "@typescript-to-lua/language-extensions": "^1.19.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "luaparse": "^0.3.1",
    "playwright": "1.47.0",
    "semver": "^7.6.3",
    "ts-pattern": "^5.6.0",
    "typescript": "^5.7.3",
    "typescript-language-server": "^4.3.3",
    "yargs": "^17.7.2"
  }
}
